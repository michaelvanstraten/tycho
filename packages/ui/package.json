{
  "name": "@tycho/ui",
  "version": "0.0.1",
  "private": false,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles": "./dist/styles.css",
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build:styles": "tailwindcss -i ./styles/globals.css -o ./dist/styles.css",
    "build:components": "tsc",
    "dev:styles": "tailwindcss -i ./styles/globals.css -o ./dist/styles.css --watch",
    "dev:components": "tsc --watch"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.543.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2"
  }
}
